<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openimmunizationsoftware.dqa.db.model">
  <class name="VaccineCvx" table="dqa_vaccine_cvx">
    <id name="cvxId" column="cvx_id" type="int">
      <generator class="assigned"/>
    </id>
    <property name="cvxCode" column="cvx_code" type="string"/>
    <property name="cvxLabel" column="cvx_label" type="string"/>
    <property name="validStartDate" column="valid_start_date" type="date"/>
    <property name="useStartDate" column="use_start_date" type="date"/>
    <property name="useEndDate" column="use_end_date" type="date"/>
    <property name="validEndDate" column="valid_end_date" type="date"/>
    <property name="useMonthStart" column="use_month_start" type="int"/>
    <property name="useMonthEnd" column="use_month_end" type="int"/>
    <property name="conceptType" column="concept_type" type="string"/>
  </class>
</hibernate-mapping>
